{
  "manifest_version": 2,
  "name": "uProbe",
  "description": "Blocking diagnosis",
  "version": "0.0.1",
  "app": {
    "background": {
      "scripts": ["background.js", "uprobe.js", "logencrypt.js",
      "third_party/pidCrypt/pidcrypt.js", "third_party/pidCrypt/pidcrypt_util.js"]
    }
  },
  "sockets": {
    "udp": {
      "send": ":3333",
      "bind": ":5758"
    } 
  },
  "permissions": [
    { "socket": ["tcp-connect", "tcp-listen", "udp-bind", "udp-send-to"] }
  ]
}
